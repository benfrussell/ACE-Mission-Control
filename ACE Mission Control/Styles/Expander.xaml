    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:muxc="using:Microsoft.UI.Xaml.Controls">

    <Style x:Key="ACEExpander" TargetType="muxc:Expander">
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
